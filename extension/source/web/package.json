{
  "name": "extension",
  "version": "3.2.1",
  "description": "Extension wallet",
  "private": true,
  "repository": "https://github.com/syscoin/pali-wallet",
  "license": "MIT",
  "engines": {
    "node": ">=10.0.0",
    "yarn": ">= 1.0.0"
  },
  "browser": {
    "fs": false,
    "path": false,
    "os": false
  },
  "scripts": {
    "dev:chrome": "cross-env NODE_ENV=development cross-env TARGET_BROWSER=chrome webpack --watch",
    "dev:firefox": "cross-env NODE_ENV=development cross-env TARGET_BROWSER=firefox webpack --watch",
    "build:chrome": "cross-env NODE_ENV=production cross-env TARGET_BROWSER=chrome webpack",
    "build:firefox": "cross-env NODE_ENV=production cross-env TARGET_BROWSER=firefox webpack",
    "build": "yarn run build:chrome && yarn run build:firefox",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "test": "jest",
    "test:e2e:chrome": "SELENIUM_BROWSER=chrome node e2e/run-all.js",
    "test:e2e:chrome:silent": "SELENIUM_BROWSER=chrome ENABLE_CHROME_LOGGING=false node e2e/run-all.js",
    "postinstall": "cd ../../ && npx patch-package"
  },
  "dependencies": {
    "@babel/runtime": "^7.10.2",
    "@ledgerhq/hw-transport-webhid": "^5.51.1",
    "@n8tb1t/use-scroll-position": "^2.0.3",
    "@types/lodash": "^4.14.162",
    "@types/qrcode.react": "^1.0.1",
    "@types/react-router-dom": "^5.1.5",
    "@types/uuid": "^8.3.0",
    "@types/yup": "^0.29.9",
    "advanced-css-reset": "^1.1.1",
    "clsx": "^1.1.1",
    "date-fns": "^2.16.1",
    "emoji-log": "^1.0.2",
    "eth-data-decoder": "^0.0.2",
    "ethereum-input-data-decoder": "^0.3.5",
    "ethers": "^5.0.32",
    "etherscan-api": "^10.0.5",
    "extensionizer": "^1.0.1",
    "framer-motion": "^4.1.17",
    "lodash": "^4.17.20",
    "net": "^1.0.2",
    "query-string": "^7.0.1",
    "react": "^16.13.1",
    "react-modal": "^3.14.3",
    "react-spring": "^8.0.27",
    "rxjs": "^6.6.7",
    "sass": "^1.35.1",
    "tls": "^0.0.1",
    "url-loader": "^4.1.1",
    "uuid": "^8.3.2",
    "web3": "^1.6.0",
    "webext-base-css": "^1.1.0",
    "webext-redux": "^2.1.7",
    "webextension-polyfill-ts": "^0.16.0",
    "yup": "^0.29.3"
  },
  "devDependencies": {
    "@abhijithvijayan/eslint-config": "^1.4.1",
    "@abhijithvijayan/tsconfig": "^1.2.0",
    "@babel/core": "^7.10.2",
    "@babel/plugin-proposal-class-properties": "^7.10.1",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.13.8",
    "@babel/plugin-proposal-object-rest-spread": "^7.10.1",
    "@babel/plugin-proposal-optional-chaining": "^7.13.12",
    "@babel/plugin-transform-runtime": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-react": "^7.10.1",
    "@babel/preset-typescript": "^7.15.0",
    "@testing-library/react-hooks": "^7.0.2",
    "@types/jest": "^27.0.2",
    "@types/node": "^15.0.3",
    "@types/query-string": "^6.3.0",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@types/webpack": "^4.41.17",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "autoprefixer": "^9.8.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^27.2.1",
    "babel-loader": "^8.1.0",
    "chromedriver": "^95.0.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.1.1",
    "cross-env": "^6.0.3",
    "css-loader": "^3.5.3",
    "css-to-xpath": "^0.1.0",
    "dotenv-webpack": "^7.0.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.4",
    "eslint-plugin-typescript-sort-keys": "^1.5.0",
